{
	"name" : "n2n/dbtext",
	"type" : "n2n-module",
	"description" : "translates textblocks from db",
	"keywords" : [
		"translation",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "GPL-3.0-or-later",
	"authors" : [{
			"name" : "Nikolai Schmid",
			"email" : "schmid@hnm.ch",
			"homepage" : "https://www.schmid.guru/"
		}, {
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://www.von-burg.net/"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": false,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"php" : ">=7.0",
		"n2n/n2n-composer-module-installer" : "^7.1",
		"n2n/n2n" : "^7.3",
		"n2n/n2n-l10n" : "^7.3",
		"n2n/n2n-persistence" : "^7.3",
        "n2n/n2n-impl-persistence-orm": "^7.3",
        "n2n/n2n-impl-persistence-meta": "^7.3",
		"n2n/rocket" : "^4"
	},
	"require-dev" : {
        "n2n/n2n-test" : "^7.3",
        "phpunit/phpunit" : "^9.5.25"
    },
	"autoload" : {
		"psr-4" : {
			"dbtext\\" : "src/app/dbtext"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"dbtext\\" : "src/app/dbtext"
		}
	}
}
