{
	"name" : "n2n/dbtext",
	"type" : "n2n-module",
	"description" : "translates textblocks from db",
	"keywords" : [
		"translation",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "GPL-3.0-or-later",
	"authors" : [{
			"name" : "Nikolai Schmid",
			"email" : "schmid@hnm.ch",
			"homepage" : "https://www.schmid.guru/"
		}, {
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://www.von-burg.net/"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor"
	},
	"require" : {
		"php" : ">=7.0",
		"n2n/n2n-composer-module-installer" : "^7.1",
		"n2n/n2n" : "^7.2.33",
		"n2n/n2n-l10n" : "^7.2.33",
		"n2n/n2n-persistence" : "^7.2.33",
		"n2n/rocket" : "^3"
	},
	"autoload" : {
		"psr-4" : {
			"dbtext\\" : "src/app/dbtext"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"dbtext\\" : "src/app/dbtext"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5"
	}
}