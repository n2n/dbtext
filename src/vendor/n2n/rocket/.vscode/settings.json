{
    "files.exclude": {
        "**/anglgui/e2e**": true,
        "**/anglgui/node_modules**": true,
        "**/anglgui/.*": true,
      },
    "search.exclude": {
        "**/assets/rocket/**angl**": true,
        // "anglgui/e2e**": true,
        // "anglgui/node_modules**": true,
        "**.css": true,
        "**/theme-20**": true,
      },

    "liveSassCompile.settings.formats":[
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/src/public/assets/rocket/css"
        }
    ],
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**",
        "**/font-awesome/**",
         "**/n2nutil-bootstrap/**",
         "**/hangar/**",
         "**/bootstrap/**",
         "**/theme-20/**",
         "**/theme-30/tag.scss"
    ],
    "liveSassCompile.settings.autoprefix": [
        "> 1%",
        "last 2 versions"
    ]
}
